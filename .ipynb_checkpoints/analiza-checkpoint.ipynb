   "source": [
    "wyn = {}\n",
    "with open(\"./wyn.csv\", \"r\") as f:\n",
    "    for l in f.readlines():\n",
    "        index, points = l.split(\",\")\n",
    "        if points != \"\\n\":\n",
    "            points_sum = sum(int(x) for x in points.split(\" \"))\n",
    "        else:\n",
    "            points_sum = 0\n",
    "        wyn[index] = points_sum"
   ]
